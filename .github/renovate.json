{
  "extends": [
    "config:base"
  ],
  "github-actions": {
    "fileMatch": ["renovate.json"]
  },
  "packageRules": [
    {
      "matchPackagePatterns": [ "javafx" ],
      "separateMajorMinor": false,
      "groupName": "javafx"
    }
  ],
  "dependencyDashboard": true,
  "branchConcurrentLimit": 50,
  "prHourlyLimit": 0,
  "prConcurrentLimit": 50,
  "automerge": true,
  "automergeType": "branch"
}
